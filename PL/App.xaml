             xmlns:local="clr-namespace:PL"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <local:WorkerLevel x:Key="WorkerLevelKey" />
        <local:Level x:Key="LevelKey" />
        <local:TaskStatus x:Key="TaskStatusKey" />
